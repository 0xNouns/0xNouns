pragma solidity ^0.8.6;

import { NounsIntegrationBase } from './NounsIntegrationBase.sol';
import { INounsMetadataIntegration } from './interfaces/INounsMetadataIntegration.sol';

contract NounsMetadataIntegrationBase is NounsIntegrationBase {
    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        return interfaceId == type(INounsMetadataIntegration).interfaceId || super.supportsInterface(interfaceId);
    }
}
